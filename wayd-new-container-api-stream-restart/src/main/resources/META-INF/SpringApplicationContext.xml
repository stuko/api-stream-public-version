<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


	<bean id="Kronos" class="com.stuko.kronos.impl.KronosPlatformImpl">
<!-- 	 	<property name="containerManager" ref="ContainerManager" /> -->
	 	<property name="genericContainerManager" ref="GenericContainerManager" />
 	 	<property name="lockManager" ref="LockManager" />
	 	<property name="systemStatusManager" ref="SystemStatusManager" />
		<property name="engineStatusManager" ref = "EngineStatusManager" />
	 	<property name="configurationManager" ref="ConfigurationManager" />
	 	<property name="threadPoolManager" ref="ThreadPoolManager" />
	 	<property name="monitoringManager" ref = "MonitoringManager" />
	 	<property name="thirdPartyEngineManager" ref = "ThirdPartyEngineManager" />
	 	<property name="profileManager" ref = "ProfileManager" />
		<property name="name" value="Kronos" />
	</bean>
   
   
   
</beans>